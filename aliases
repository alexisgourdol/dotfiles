# Get External IP / Internet Speed
alias myip="curl https://ipinfo.io/json" # or /ip for plain-text ip
alias speedtest="curl -s https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest.py | python -"

# Quickly serve the current directory as HTTP
alias serve='ruby -run -e httpd . -p 8000' # Or python -m SimpleHTTPServer :)

# Shortcut to launch a notebook
alias juno="jupyter notebook"

# Shortcut to /Users/alexisgourdol/code/alexisgourdol folder
alias ln="cd /Users/ln/code && pwd && ls"

# Shortcut to launch vs code
alias nano='/usr/local/bin/nano'
alias tree="tree -C"

##
alias brew="/opt/homebrew/bin/brew"

#To get the block height of the latest blocks, read the output of this command
alias last15="lumd query block --node \"https://node0.mainnet.lum.network:443/rpc\""
alias bondedtokens="lumd query staking pool --node \"https://node0.mainnet.lum.network:443/rpc\""
